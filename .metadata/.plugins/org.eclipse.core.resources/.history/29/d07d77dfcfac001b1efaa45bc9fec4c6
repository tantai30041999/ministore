package util;

import dao.UserDAO;

public class GenerateID {
    public String generateIDUser() {
    	String idUser = "";
    	String idUserLast = UserDAO.getUserIdLast();
    	for(int i = idUserLast.length() -1 ; i >=0; i--) {
    		if(idUserLast.charAt(i) =='0') {
    			int number = Integer.parseInt(idUserLast.substring(i));
    			idUser+= idUser.substring(0, i+1)+(++number);
    			break;
    		}
    		
    	}
    	return idUser;
    	
    }
    public String generateIDStore() {
    	String idStore = "";
    	String idStoreLast = UserDAO.getUserIdLast();
    	for(int i = idUserLast.length() -1 ; i >=0; i--) {
    		if(idUserLast.charAt(i) =='0') {
    			int number = Integer.parseInt(idUserLast.substring(i));
    			idUser+= idUser.substring(0, i+1)+(++number);
    			break;
    		}
    		
    	}
    	return idUser;
    }
    public static void main(String[] args) {
		
	}
}
